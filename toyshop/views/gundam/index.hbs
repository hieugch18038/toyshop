<table class="table table-bordered bg-light mt-3 mb-3 text-center">
    <tr>
        <th class="text-primary">Gundam list</th>
        <th>
            <a class="btn btn-success">Add</a>
        </th>
    </tr>
    <tr>
        <th>Name</th>
        <th>Brand</th>
        <th>Grade</th>
        <th>Age</th>
        <th>Made in</th>
        <th>Image</th>
    </tr>

    {{#each gundams}}
    <tr>
        <th>{{name}}</th>
        <th>{{brand}}</th>
        <th>{{grade}}</th>
        <th>{{age_require}}</th>
        <th>{{made_in}}</th>
        <th><img src="{{image}}" width="100" height="100"></th>
    </tr>
    {{/each}}
</table>