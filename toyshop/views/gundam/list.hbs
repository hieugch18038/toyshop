<div>bs5-card-group</div>
<div class="card-group text-center">
    <div class="row">
        {{#each gundam}}
        <div class="card col-md-3 m-3">
            
            <img class="card-img-top" src="{{image}}" width="200" height="200">
            <div class="card-body">
                <h4 class="card-title">{{name}}</h4>
                <p class="card-text">{{brand}}</p>
            </div>
        </div>
        {{/each}}
    </div>

</div>
